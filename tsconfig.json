{
  "compilerOptions": {
    "module": "None",
    "target": "es2017",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": false,
    "jsx": "react-jsx",
    "outDir": "dist",
    "baseUrl": ".",
    "incremental": false,
    "lib": ["dom", "esnext"],
    "experimentalDecorators": true,
    "sourceMap": true,
  },
  "include": ["src/**/*"],
}
